<%= link_to profiles_my_dashboard_path(current_user), class:"btn-back lower" do  %>
  <i class="fas fa-arrow-circle-left arrow-leaderboard"></i>
<% end %>
<h1 class="text-center">Leaderboard</h1>
<div class="container">
    <% @users.each do |user| %>
  <div class="user">
      <strong> <%= cl_image_tag user.photo.key, class: "avatar-small" %> <%= link_to user.username, user_path(user) %> </strong> ⭐️ Score: <%= user.score %> <p> "<%= user.bio %>" </p>
  </div>
    <% end %>
</div>
